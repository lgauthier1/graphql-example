generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  username  String
  confirmed Boolean  @default(false)
  password  String
  createAt  DateTime @default(now())
  updatedAt DateTime @default(now())
}
